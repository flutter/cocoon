{
  "name": "no-response",
  "version": "0.0.0",
  "private": true,
  "description": "A GitHub Action that closes Issues where the author hasn't responded to a request for more information",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx ncc build ./src/main.ts",
    "ci": "npm run format-check && npm run lint && npm test",
    "format": "prettier --write **/*.ts **/*.md **/*.yaml **/*.yml",
    "format-check": "prettier --check **/*.ts **/*.md **/*.yaml **/*.yml",
    "lint": "npx eslint src/**/*.ts",
    "start": "npx ncc run ./src/main.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flutter/cocoon/gh_actions/third_party/no-response"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "lee-dohm",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/github": "^6.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "scramjet": "^4.37.0"
  },
  "devDependencies": {
    "@octokit/webhooks-types": "^7.3.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^20.8.10",
    "@vercel/ncc": "^0.38.1",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-jest": "^27.6.0",
    "extract-pr-titles": "^1.1.0",
    "jest": "^27.0.6",
    "js-yaml": "^4.1.0",
    "prettier": "3.0.3",
    "ts-jest": "^27.0.3",
    "typescript": "^4.9.5"
  }
}
